{
  "version": 2,
  "builds": [
    {
      "src": "/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    { "src": "/test", "dest": "/index.js" },
    { "src": "/registro", "methods": ["POST", "GET", "PATCH", "PUT", "DELETE"], "dest": "/index.js"},
    { "src": "/electrodomesticos/(?<id>[^/]+)", "methods": ["POST", "GET"], "dest": "/index.js"},
    { "src": "/ajustes/(?<id>[^/]+)", "methods": ["POST", "GET", "PATCH", "PUT", "DELETE"], "dest": "/index.js" }
  ]
}
